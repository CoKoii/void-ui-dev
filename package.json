{
  "name": "void-design-vue",
  "version": "1.0.6",
  "description": "A Vue 3 UI Component Library",
  "type": "module",
  "main": "./dist/void-design-vue.umd.cjs",
  "module": "./dist/void-design-vue.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/void-design-vue.js",
      "require": "./dist/void-design-vue.umd.cjs"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./style.css": "./dist/style.css",
    "./dist/*": "./dist/*"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "./src/styles/*"
  ],
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "library"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoKoii/void-ui-dev.git"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:lib": "run-s clean build:vite build:types build:post",
    "build:vite": "vite build --config vite.lib.config.ts",
    "build:types": "vue-tsc --project tsconfig.build.json",
    "build:post": "node scripts/post-build.mjs",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build:lib",
    "lazy": "node scripts/auto-publish.mjs"
  },
  "dependencies": {
    "@ant-design/colors": "^7.2.1",
    "sass": "^1.91.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/glob": "^9.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "glob": "^11.0.3",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  }
}
